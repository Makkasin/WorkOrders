		<query>SELECT
ЗаказНаряд,
Док.Номер,
Док.Контрагент,
Док.ДатаИспытания,
Спр.ссылка Оборудование,
Спр.ссылка ОборудованиеГрп,
Спр.родитель Группа,
Спр.Диаметр,
Спр.Давление,
СпрРод.родитель Группа0,
MAX(ISNULL(Рег.Цена,ISNULL(Рег0.цена,ЦенаРеестра))) Цена,
SUM(НачОст) НачОст,
SUM(КонОст) КонОст,
SUM(Приход) Приход,
SUM(Расход) Расход,
SUM(Работа) Работа,
SUM(Реестр) Реестр,
SUM(КонОст)*MAX(ISNULL(Рег.Цена,ISNULL(Рег0.цена,ЦенаРеестра))) СумОст,
SUM(Работа)*MAX(ISNULL(Рег.Цена,ISNULL(Рег0.цена,ЦенаРеестра))) СумРабота,
SUM(Реестр*ЦенаРеестра) СумРеестр



FROM
(
ВЫБРАТЬ
	ОстатокРООстаткиИОбороты.ЗаказНаряд КАК ЗаказНаряд,
	Док.Оборудование КАК Оборудование,
	ОстатокРООстаткиИОбороты.КолНачальныйОстаток КАК НачОст,
	ОстатокРООстаткиИОбороты.КолКонечныйОстаток КАК КонОст,
	ОстатокРООстаткиИОбороты.КолПриход КАК Приход,
	ОстатокРООстаткиИОбороты.КолРасход КАК Расход,
	0 Работа,
	0 Реестр,
	0 ЦенаРеестра
ИЗ
	РегистрНакопления.ОстатокРО.ОстаткиИОбороты(&amp;Дт1,&amp;Дт2) КАК ОстатокРООстаткиИОбороты

INNER JOIN Документ.роЗаказНаряд Док ON Док.ссылка = ОстатокРООстаткиИОбороты.ЗаказНаряд
	
UNION ALL


ВЫБРАТЬ
	РаботаРООстатки.ЗаказНаряд КАК ЗаказНаряд,
	РаботаРООстатки.Оборудование КАК Оборудование,
	0 КАК КолНачальныйОстаток,
	0 КАК КолКонечныйОстаток,
	0 КАК КолПриход,
	0 КАК КолРасход,
	КолОстаток КАК Работа,
	0 Реестр,
	0 ЦенаРеестра
ИЗ
	РегистрНакопления.РаботаРО.Остатки(&amp;Дт2) КАК РаботаРООстатки
	
	
UNION ALL

ВЫБРАТЬ
	РаботаРООстатки.ЗаказНаряд КАК ЗаказНаряд,
	РаботаРООстатки.Оборудование КАК Оборудование,
	0 КАК КолНачальныйОстаток,
	0 КАК КолКонечныйОстаток,
	0 КАК КолПриход,
	0 КАК КолРасход,
	0 КАК Работа,
	РаботаРООстатки.Кол КАК Реестр,
	Цена ЦенаРеестра
ИЗ
	РегистрНакопления.РаботаРО КАК РаботаРООстатки
ГДЕ
	РаботаРООстатки.ВидДвижения = Значение(ВидДвиженияНакопления.Расход)
	and период &gt;= &amp;Дт1
	and период &lt;= &amp;Дт2
	
	
) WWW	
INNER JOIN Документ.роЗаказНаряд Док ON Док.ссылка = WWW.ЗаказНаряд
INNER JOIN Справочник.ро_Оборудование Спр ON Спр.ссылка = WWW.Оборудование
LEFT OUTER JOIN Справочник.ро_Оборудование СпрРод ON СпрРод.ссылка = Спр.Родитель
	
LEFT OUTER JOIN РегистрСведений.Прайс.СрезПоследних(&amp;Дт2) Рег ON WWW.ЦенаРеестра = 0
														  и Рег.Контрагент   = ЗаказНаряд.Контрагент
				                                          и  Рег.Оборудование = Спр.родитель 
				                                          и (Рег.ДиаметрОт   &lt;= Спр.Диаметр  или Рег.ДиаметрОт  = 0)
				                                          и (Рег.ДиаметрДо   &gt;= Спр.Диаметр  или Рег.ДиаметрДо  = 0)
				                                          и (Рег.ДавлениеОт  &lt;= Спр.Давление или Рег.ДавлениеОт = 0)
				                                          и (Рег.ДавлениеДо  &gt;= Спр.Давление или Рег.ДавлениеДо = 0)

	
LEFT OUTER JOIN РегистрСведений.Прайс.СрезПоследних(&amp;Дт2) Рег0 ON WWW.ЦенаРеестра = 0
														  и  Рег0.Контрагент   = Значение(Справочник.Контрагенты.ПустаяСсылка)
				                                          и  Рег0.Оборудование = Спр.родитель 
				                                          и (Рег0.ДиаметрОт   &lt;= Спр.Диаметр  или Рег.ДиаметрОт  = 0)
				                                          и (Рег0.ДиаметрДо   &gt;= Спр.Диаметр  или Рег.ДиаметрДо  = 0)
				                                          и (Рег0.ДавлениеОт  &lt;= Спр.Давление или Рег.ДавлениеОт = 0)
				                                          и (Рег0.ДавлениеДо  &gt;= Спр.Давление или Рег.ДавлениеДо = 0)
GROUP BY
ЗаказНаряд,
Док.Контрагент,
Док.Номер,
Док.ДатаИспытания,
Спр.ссылка ,
Спр.родитель ,
Спр.Диаметр,
Спр.Давление,
СпрРод.родитель 

</query>
